{
  "name": "trialsfinder",
  "version": "1.0.0",
  "description": "Clinical trials finder application",
  "main": "index.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:analyze": "cross-env NODE_ENV=production webpack --mode production --env ANALYZE=true",
    "build:stats": "cross-env NODE_ENV=production webpack --mode production --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json dist",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern=\"unit\" --passWithNoTests",
    "test:integration": "jest --testPathPattern=\"integration\" --passWithNoTests",
    "test:performance": "jest --testPathPattern=\"performance\" --runInBand --passWithNoTests --maxWorkers=1",
    "test:bundle": "node src/test/performance/analyze-bundle.js",
    "test:lighthouse": "node src/test/performance/lighthouse-test.js",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --testPathPattern=memory --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "serve": "serve -s dist -l 3000",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully!'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "date-fns": "^3.6.0",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^7.1.0",
    "immer": "^10.1.1",
    "preact": "^10.26.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-router-dom": "^6.26.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.27.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.5",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chrome-launcher": "^1.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.1.0",
    "eslint": "^8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lighthouse": "^11.3.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.2.4",
    "puppeteer": "^21.5.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.3",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}